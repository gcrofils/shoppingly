%h1= itinerary_title(@itinerary)
= semantic_form_for @itinerary, method: :put, url: wizard_path do |f|
  = f.input :title
  %button.btn.btn-default.bootbox-photo{type: 'button', data: {url: ckeditor.pictures_path}}
    %i.fa.fa-picture-o
    Select
  = f.input :description, :as => :ckeditor
  = render partial: 'actions', locals: {f: f, current_step: @current_step}
  

